{
	"handler_timeout":{
		"/im.status/ping":{
			"GET":"200ms",
			"CRPC":"200ms",
			"GRPC":"200ms"
		}
	},
	"web_path_rewrite":{
		"GET":{
			"/origin/url":"/im.exampleservice/examplemethod"
		}
	},
	"handler_rate":{
		"/im.exampleservice/examplemethod":[{
			"methods":["GET","GRPC","CRPC"],
			"max_rate":10,
			"period":1,
			"rate_type":"path"
		},{
			"methods":["GET","GRPC","CRPC"],
			"max_rate":10,
			"period":1,
			"rate_type":"token"
		}]
	},
	"accesses":{
		"/im.exampleservice/examplemethod":[{
			"methods":["GET","GRPC","CRPC"],
			"accesses":{
				"accessid":"accesskey"
			}
		}]
	},
	"token_secret":"test",
	"session_token_expire":"24h",
	"service":{
		"max_user_relation":50,
		"max_group_member":100
	}
}
